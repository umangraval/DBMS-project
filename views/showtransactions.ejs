<table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Serial No</th>
        <th scope="col">Token</th>
        <th scope="col">Price</th>
        <th scope="col">Property Id</th>
        <th scope="col">Date</th>
      </tr>
    </thead>
    <tbody>
      <% var c=1 %>
            <% data["rows"].forEach(function(trx){ %>
        <tr>
        <th scope="row"><%=c%></th>
        <td><%=trx.token%></td>
        <td><%=trx.price%></td>
        <td><%=trx.propertyid%></td>
        <td><%=trx.date%></td>
      </tr>
      <% c++ %>
      <% }) %>
    </tbody>
</table>
