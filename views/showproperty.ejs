<div> <h1>showing details</h1></div>
<div>
    <% foundinfo["rows"].forEach(function(property){ %>
        <li><%=property.name%></li>
        <li><%=property.place %></li>
        <li><%=property.cost %></li>
    <% }) %>
</div>
<form method="post" action="/buy">
    <input type="submit" value="buy">
</form>
<div>
        <h2>REVIEWS</h2>
        <% reviews["rows"].forEach(function(review){ %>
            <li><%=review.comment%></li>
            <% }) %>
    </div>
    <div>
        <h2>write a review</h2>
        <form method="post" action="/review">
        <input type="text" name="comment" placeholder="Review">
        <input type="submit" value="submit">
        </form>
    </div>

    <button type="button"><a href="/rent">rent</a></button>
    <button type="button"><a href="/property">back</a></button>